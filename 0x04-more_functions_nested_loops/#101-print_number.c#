#include <stdio.h>
#include "holberton.h"
/**
 *print_number - print integer with putchar
 *@n: integer
 */
void print_number(int n)
{
	int d = 10;

	int i;

	for (i = 1; d < n; i++)
	{
		if (i <= n)
		{
			
u = n % d;
			d = n / d;
			d = n / d;		_putchar(i + '0');
			d = 10 * d;
	}
	_putchar('\n');
}
