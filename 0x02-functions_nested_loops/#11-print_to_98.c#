#include "holberton.h"
/**
 *times_table - Print multiplication table
 *
 */

void print_to_98(int n);
{
	int a, b;

 	for (a = 0; a <= 9; a++)



	{
		for (j = 0; j <= 9; j++)
		{
			n = i * j;
			d = n / 10;
			u = n % 10;
			if (n < 10)
			{
				if (j != 0)
				{
					_putchar(' ');
					_putchar(' ');
				}
				_putchar('0' + u);
			}

			else
			{
				_putchar(' ');
				_putchar('0' + d);
				_putchar('0' + u);
			}
			if (j != 9)
			{
			_putchar(',');
			}
		}
		_putchar('\n');
	}
}
